		{
			"name": "Criar pátio",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status 201 or 200\", () => {",
							"  pm.expect(pm.response.code).to.be.oneOf([200, 201]);",
							"});",
							"",
							"const json = pm.response.json();",
							"",
							"pm.test(\"Response has yard id\", () => {",
							"  pm.expect(json).to.have.property(\"id\");",
							"});",
							"",
							"pm.test(\"Yard has correct capacity\", () => {",
							"  pm.expect(json.capacity).to.eql(120);",
							"});",
							"",
							"pm.environment.set(\"yardId\", json.id);"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"name\": \"Yard QA\",\n  \"ownerName\": \"Guilherme Maggiorini\",\n  \"capacity\": 120\n}",
