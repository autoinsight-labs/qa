		{
			"name": "Forecast de capacidade",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Forecast retrieved\", () => {",
							"  pm.expect(pm.response.code).to.equal(200);",
							"});",
							"",
							"const json = pm.response.json();",
							"",
							"pm.test(\"Forecast contains points array\", () => {",
							"  pm.expect(json).to.have.property(\"points\");",
							"  pm.expect(json.points).to.be.an(\"array\").that.is.not.empty;",
							"});",
							"",
							"pm.test(\"Each point has occupancyRatio\", () => {",
							"  json.points.forEach(p => {",
							"    pm.expect(p).to.have.property(\"occupancyRatio\");",
							"  });",
							"});"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
